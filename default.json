      "server_name": [
        "server\uD83D\uDC081"
      ],
      "listen": [
        "8082",
        "localhost:8080"
      ],
      "root": "./",
      "error_page": {
        "404": "/404.html",
        "500": "/50x.html",
        "502": "/50x.html",
        "503": "/50x.html",
        "504": "/50x.html"
      },
      "location": {
        "/": {
          "root": "/usr/share/nginx/html",
          "allow_methods": [
            "GET",
            "POST"
          ],
          "return": "200 \"Hello World\n\""
        }
      }
    },
